FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      voltD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{

    leftBoundary
    {
        type            zeroGradient;
    }

    rightBoundary
    {
        type            zeroGradient;
    }

    bottomBoundary
    {
        type            zeroGradient;
    }

    anode
    {
        type            fixedValue;
        value           uniform 10000;
    }

    dielectric_to_region0
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            voltA;
        kappaMethod     lookup;
        kappa           epsilonD;
    }

    cathode
    {
        type            fixedValue;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }

}

