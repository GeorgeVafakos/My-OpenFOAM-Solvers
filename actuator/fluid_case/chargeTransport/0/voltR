FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      voltR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{

    upperBoundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    leftBoundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    rightBoundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    highVoltage
    {
        type            zeroGradient;
    }

    region0_to_dielectric
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }

}

