FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // Dielectric Zone
    {
        name    dielSet;
        type    cellSet;
        action  new;
        source  boxToCell;
        sourceInfo
        {
            box (-10.0 -0.002 -1.0) (10.0 0.0 1.0);
        }
    }
    {
        name    dielectric;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set dielSet;
        }
    }

    // Insulator Zone
    {
        name    insSet;
        type    cellSet;
        action  new;
        source  boxToCell;
        sourceInfo
        {
            box (-10.0 -10.0 -1.0) (10.0 -0.002 1.0);
        }
    }
    {
        name    insulator;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set insSet;
        }
    }

    // // Streamer head
