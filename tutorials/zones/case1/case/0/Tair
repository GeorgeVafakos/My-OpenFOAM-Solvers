FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Tair;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            fixedValue;
        value           uniform 0;
    }

    leftWall
    {
        type            fixedValue;
        value           uniform 0;
    }

    bottomWall
    {
        type            fixedValue;
        value           uniform 0;
    }

    region0_to_solid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            Tsolid;
        kappaMethod     lookup;
        kappa           kappaAir;
    }

    frontAndBack
    {
        type            empty;
    }
}

