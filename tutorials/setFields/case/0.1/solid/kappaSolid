FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1/solid";
    object      kappaSolid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    topWall
    {
        type            calculated;
        value           uniform 1;
    }
    rightWall
    {
        type            calculated;
        value           uniform 1;
    }
    bottomWall
    {
        type            calculated;
        value           uniform 1;
    }
    frontAndBack
    {
        type            empty;
    }
    solid_to_air
    {
        type            calculated;
        value           uniform 1;
    }
}


